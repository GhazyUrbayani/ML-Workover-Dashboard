{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "dashboard_data.json",
      "use": "@vercel/static"
    },
    {
      "src": "*.joblib",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/dashboard_data.json",
      "dest": "/dashboard_data.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
